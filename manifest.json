{
    "manifest_version": 3,
    "name": "Tab Title Fetcher",
    "version": "1.0",
    "description": "Fetches the title of the current tab.",
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/picker_16.png"
        
      }
    }
  }
  